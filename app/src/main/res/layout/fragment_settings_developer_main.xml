<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:id="@+id/setting_background"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    xmlns:card_view="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <ScrollView
        android:id="@+id/settingAct_scrollView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scrollbarSize="@dimen/scrollbar_width">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <com.zaen.testly.views.settings.SettingSwitchTitleonlyView
                android:id="@+id/pref_dev_devbar"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:settingPreferenceKey="@string/pref_dev_devbar"
                app:settingTitle="@string/pref_dev_devbar_title"/>
        </LinearLayout>
    </ScrollView>
</LinearLayout>